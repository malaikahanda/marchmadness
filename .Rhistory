cat(dark_n, light_n, "\n")
}
# loop through quantiles
unique_models = NULL
for (n in 2:20) {
q = quantile(colors$l, probs = seq(0, 1,length.out = n))
dark = filter(colors, l < q[2])
light = filter(colors, l > q[n - 1])
dark_n = length(unique(dark$model))
light_n = length(unique(light$model))
unique_models = rbind(unique_models, c(dark_n, light_n))
}
